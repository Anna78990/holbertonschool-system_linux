
    client_ip = (char *)malloc(sizeof(char) * INET_ADDRSTRLEN);
    inet_ntop(AF_INET, &(client_addr.sin_addr), client_ip, INET_ADDRSTRLEN);
    printf("Client connected: %s\n", client_ip);
    free(client_ip);

    handle_request(client_fd);

    close(client_fd);
    close(server_fd);
    return EXIT_SUCCESS;
